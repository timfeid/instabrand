{
  "name": "@instabrand/server",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "license": "MIT",
  "prisma": {
    "schema": "../../packages/data/prisma/schema.prisma",
    "seed": "ts-node prisma/data.ts"
  },
  "scripts": {
    "build": "node esbuild.js",
    "docker-build": "cp ../../../yarn.lock . && docker build -t fdl:latest . && rm -f ./yarn.lock",
    "start": "test -f dist/server.js && node dist/server.js",
    "dev": "tsnd --respawn --transpile-only src/server.ts",
    "lint": "eslint ./src",
    "reset-test-db": "./reset-dev.sh",
    "test:integration": "TEST_TYPE=integration yarn reset-test-db && jest ./__tests__/integration/*.test.ts",
    "test:unit": "TEST_TYPE=unit jest ./__tests__/unit/*.test.ts"
  },
